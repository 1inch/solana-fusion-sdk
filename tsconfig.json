{
    "extends": "@1inch/tsconfig",
    "compilerOptions": {
        "target": "ES2024",
        "lib": ["ES2024"],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./src",
        "removeComments": false
    },
    "include": ["./src/**/*.ts", "./src/**/*.json", "./scripts/**/*.ts", "./tests/**/*.ts"]
}
